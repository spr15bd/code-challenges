class CircularQueue {
  constructor(size) {

    this.queue = [];
    this.read = 0;
    this.write = 0;
    this.max = size - 1;

    while (size > 0) {
      this.queue.push(null);
      size--;
    }
  }

  print() {
    return this.queue;
  }

  enqueue(item) {
    // Only change code below this line
    this.queue[this.write] = item
      
    if (this.queue[this.write] === item) {
      this.write++
      if (this.write > this.queue.length) {
        this.write = 0
      }
      return item
    } else {
        if (this.write > this.queue.length) {
        this.write = 0
      }
      return null
    }
    // Only change code above this line
  }

  dequeue() {
    // Only change code below this line
    let itemToReturn = null
    if (this.read < this.write) {
      itemToReturn = this.queue[this.read]
      this.queue[this.read] = null
      this.read++
      return itemToReturn
    } else {
      return null
    }
    // Only change code above this line
  }
}

let c = new CircularQueue(3)
console.log("c before enqueuing is ", c)
c.enqueue('h')
c.enqueue('v')
c.enqueue('w')
console.log("c is now ", c)
c.dequeue()
console.log("c after dequeing is ", c)
