function bubbleSort(array) {
  // Only change code below this line
  array.forEach(num=>{
    array.forEach((item,idx)=>{
      if (array[idx+1] < item) {
        let temp
        temp = item
        array[idx] = array[idx+1]
        array[idx+1] = temp
      }
    })
  })
  return array;
  // Only change code above this line
}

bubbleSort([3,2,1])
