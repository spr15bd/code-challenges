function selectionSort(array) {
  // Only change code below this line
  let pos = 0
  while (pos < array.length) {
    let minValue = Math.min(...array)
    console.log("minValue: ", minValue)
    let minValueIdx = array.indexOf(minValue)
    console.log("minValueIdx: ", minValueIdx)
    let temp = array[minValueIdx]
    console.log("temp: ", temp)
    array[minValueIdx] = array[pos]
    console.log(minValueIdx,"th value in array: ", array[minValueIdx])
    array[pos] = temp
    console.log(array,"[",pos,"]:", array[pos])
    pos++
  }
    
  return array;
  // Only change code above this line
}

console.log("result: ", selectionSort([1,7,6,5,9]))
