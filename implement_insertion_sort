function insertionSort(array) {
  // Only change code below this line
  let sortedArray = []
  sortedArray.push(array[0])

  array.shift()
  sortedArray.push(array[0])
  array.shift()
  let length = sortedArray.length - 1
  if (sortedArray[length] < sortedArray[length-1]) {
    let temp = sortedArray[length-1]

    sortedArray[length-1] = sortedArray[length]
    sortedArray[length] = temp
  }

  sortedArray.push(array[0])
  array.shift()
  length = sortedArray.length - 1


  return sortedArray;
  // Only change code above this line
}

console.log("sortedArray: ", insertionSort([7,3,2,8,5]))
