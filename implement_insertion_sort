function insertionSort(array) {
  // Only change code below this line
  let sortedArray = []
  sortedArray.push(array[0])

  array.shift()
  sortedArray.push(array[0])
  array.shift()

  let elementSorted = false
  let idx = sortedArray.length - 1
  if (sortedArray[idx] < sortedArray[idx-1]) {
    let temp = sortedArray[idx-1]

    sortedArray[idx-1] = sortedArray[idx]
    sortedArray[idx] = temp
  }

  sortedArray.push(array[0])
  array.shift()
  idx = sortedArray.length - 1
  while (!elementSorted) {
    if (sortedArray[idx] < sortedArray[idx-1]) {
      let temp = sortedArray[idx-1]

      sortedArray[idx-1] = sortedArray[idx]
      sortedArray[idx] = temp
      idx--
    } else {
      elementSorted = true
    }
  }



  return sortedArray;
  // Only change code above this line
}

console.log("sortedArray: ", insertionSort([7,3,2,8,5]))
